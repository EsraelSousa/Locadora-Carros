{
  "name": "ibiapaba-locadora",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Daniel Sobrinho <danielsobrinho.dev@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist; babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --poll --respawn --exit-child src/shared/infra/http/server.ts",
    "docker:up": "docker compose up --build -d",
    "docker:down": "docker compose down --rmi 'all' --volumes",
    "docker:rebuild": "npm run docker:down; npm run docker:up",
    "docker:restart": "docker compose restart",
    "docker:logs": "docker compose logs -f",
    "start": "node dist/shared/infra/http/server.js"
  },
  "dependencies": {
    "@prisma/client": "^5.4.2",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-handlebars": "^7.1.2",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.2",
    "mysql": "^2.18.1",
    "prisma": "^5.4.2",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/node": "^7.22.19",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-typescript": "^7.23.0",
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.3",
    "@types/express": "^4.17.18",
    "@types/express-session": "^1.17.8",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^20.7.1",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "prisma": {
    "schema": "src/shared/infra/db/prisma/schema.prisma",
    "seed": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/shared/infra/db/prisma/seeds/index.ts"
  }
}
